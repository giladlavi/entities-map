<Window x:Class="Presentor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentor"
        mc:Ignorable="d"
        Title="MainWindow" Height="807" Width="1281">
    <Canvas Height="807" Width="1281" Name="EntitiesMap">
        <Image Source="https://geology.com/world/world-map.gif" Width="{Binding Path=ActualWidth, ElementName=EntitiesMap}" Height="{Binding Path=ActualHeight, ElementName=EntitiesMap}" Stretch="Uniform" />
    </Canvas>

</Window>
